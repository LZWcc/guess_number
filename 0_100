#include<stdio.h>
#include<stdlib.h>
#include<time.h>

int main()
{
 srand(time(NULL));
 int num=rand()%100+1;
 int guess;
 printf("Guess the number between 1 to 100\n");
 
 while(guess!=num)
 {
    scanf("%d",&guess);
 if(guess==num)
 {
  printf("You guessed it right\n");
 }
 else if (guess>num)
 {
    printf("You guessed it high\n");
 }
 else
 {
    printf("You guessed it low\n");
 }
}
 

 return 0;
}
